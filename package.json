{
  "name": "linkedlist-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsnd --respawn --pretty --transpile-only --exit-child src/server.ts",
    "lint": "eslint . --ext .ts",
    "test": "npm run docker:testup && npm run prisma:testmigrate && npm run test:only",
    "test:only": "dotenv -e .env.test -- jest --verbose --detectOpenHandles",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down --volumes",
    "docker:testup": "docker compose -f docker-compose.test.yml up -d",
    "prisma:testmigrate": "dotenv -e .env.test -- npx prisma migrate deploy",
    "ci:test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "argon2": "^0.30.3",
    "connect-redis": "^6.1.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "ioredis": "^5.3.1",
    "pg": "^8.10.0"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.19",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/jest": "^29.4.4",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  }
}
